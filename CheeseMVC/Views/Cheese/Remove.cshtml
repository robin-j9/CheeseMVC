@{
}

<h1>@ViewBag.title</h1>

<form method="post">
    @foreach (Cheese cheese in ViewBag.cheeseList)
    {
        <label>
            <input type="checkbox" name="cheeseIds" value="@cheese.CheeseId" />
            @cheese.Name
        </label><br />
    }
    <input type="submit" value="Remove" />
</form>